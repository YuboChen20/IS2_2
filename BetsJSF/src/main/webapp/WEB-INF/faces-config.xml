<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://Java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>login</managed-bean-name>
		<managed-bean-class>modelo.bean.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>queryQuestion</managed-bean-name>
		<managed-bean-class>modelo.bean.QueryQuestionBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>signUp</managed-bean-name>
		<managed-bean-class>modelo.bean.SignUpBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>chat</managed-bean-name>
		<managed-bean-class>modelo.bean.ChatBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>createQuestion</managed-bean-name>
		<managed-bean-class>modelo.bean.CreateQuestionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>okAdmin</from-outcome>
			<to-view-id>/CreateQuestions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>okUser</from-outcome>
			<to-view-id>/QueryQuestion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>SignUp.xhtml</display-name>
		<from-view-id>/SignUp.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>OkSignUp</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>SignUpOk</from-outcome>
			<to-view-id>/SignUp.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryQuestion.xhtml</display-name>
		<from-view-id>/QueryQuestion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>OkSalir</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateQuestions.xhtml</display-name>
		<from-view-id>/CreateQuestions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>OkSalir</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
